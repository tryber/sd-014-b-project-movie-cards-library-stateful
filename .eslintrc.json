{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": "trybe-frontend",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react", "sonarjs"],

  "rules": {
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/label-has-associated-control": "off"
 }
}
